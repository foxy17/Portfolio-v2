{
  "extends": [
    "next",
    "next/core-web-vitals",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "prettier/prettier": ["error"]
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [["~", "./"]],
        "extensions": [".ts", ".js", ".jsx", ".tsx", ".json"]
      }
    }
  },
  "globals": {
    "PROJECT_LIST": true
  }
}